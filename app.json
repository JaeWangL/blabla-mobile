{
  "expo": {
    "name": "blablamobile",
    "slug": "blablamobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "userInterfaceStyle": "automatic",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "ios": {
      "bundleIdentifier": "com.hahahoho.blabla",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "userInterfaceStyle": "dark",
      "config": {
        "googleMapsApiKey": "AIzaSyAfljNmH-qNq3g0GHC5DxWAzIxkQptVNQY"
      },
      "infoPlist": {
        "UIBackgroundModes": ["location", "fetch"],
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app uses the location to track your location.",
        "NSLocationAlwaysUsageDescription": "This app uses the location to track your location.",
        "NSLocationWhenInUseUsageDescription": "This app uses the location to track your location."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "userInterfaceStyle": "dark",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAfljNmH-qNq3g0GHC5DxWAzIxkQptVNQY"
        }
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "packagerOpts": {
      "config": "metro.config.js"
    }
  }
}
